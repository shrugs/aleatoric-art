{
  "name": "aleatoric-art",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "new-asset": "npm run assets && npm run posters",
    "assets": "for i in ../aleatoric/assets/*.mp4; do ffmpeg -y -i \"$i\" -vcodec libx264 -pix_fmt yuv420p -profile:v baseline -c:a copy -level 3 \"static/assets/$(basename $i)\" ; done",
    "posters": "for i in ../aleatoric/assets/*.mp4; do  ffmpeg -y -i \"$i\" -frames:v 1 \"static/posters/$(basename $i).jpg\"; done"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.29.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "type": "module",
  "dependencies": {
    "@zoralabs/zdk": "^2.2.1",
    "marked": "^5.0.4",
    "marked-mangle": "^1.0.1"
  }
}
